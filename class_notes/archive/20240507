	<input type = "text" id="augend">
	<input type="button" onclick="add()">
	
	<script>
		function add() {
			var augend = document.getElementById('augend').value;
			var addend = document.getElementById('addend').value;
			var sum = Number(augend) + Number(addend)
			document.getElementById('sum').value = sum;
		}
	</script>
	

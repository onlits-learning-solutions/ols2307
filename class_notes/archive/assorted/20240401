 	Loose coupling
 	
	public static void main(String[] args)
	{
		divide();		//error
	}

 	int sum(int a, int b)
 	{
 		return a + b;
 	}
 	
 	int product()
 	{
 		return sum(5,6)		//compile-time
 	}
 	
 	Difference between byte and char data type in java.
 	
 	byte b;		-	8-bit - ascii
 	char c;		-	16-bit - unicode
 	
 	BCD, EDCDIC, ASCII (256), Unicode (65536)
 	
 	

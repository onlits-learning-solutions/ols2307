		Polymorphism
		
		Date dob = new Date();
		
		dob.getDate(11, 3, 2024);
		dob.getDate("11/03/2024");
		
		gets(), getc(), getchar()	-	Input
		puts(), putc(), putchar()	-	Output
		
		get()	-	Output
		set()	-	Input
		
		
		WAP to accept a string and find its length.
		
		#include <string.h>
		char name[30];
		name = "Aman Kumar";		//Not possible
		strcpy(name, "Aman Kumar");
		printf("%d", strlen(name));
		
		
		
		int i=0;
		while(name[i] != 0)
		{
			i++;
		}
		printf("%d", i);
		


	Copy/Paste
	Call
	
	call by value			-	copy	
	call by reference		-
	
	
	
	//call by value	
	int main(void)
	{
		int a=10, b=20;
		swap(a, b);
		printf("%d\t%d", a, b);
	}
	
	void swap(int x, int y)
	{
		int t = x;
		x = y;
		y = t;
	}
	
	
	//call by reference
	int main(void)
	{
		int a=10, b=20;
		swap(&a, &b);
		printf("%d\t%d", a, b);
	}
	
	void swap(int *x, int *y)
	{
		int t = *x;
		*x = *y;
		*y = t;
	}
	
	
